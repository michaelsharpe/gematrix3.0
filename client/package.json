  "dependencies": {
    "@expo/vector-icons": "^6.2.2",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link": "^1.0.7",
    "apollo-link-http": "^1.3.2",
    "apollo-link-state": "^0.3.1",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-react-native-web": "^0.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-expo": "^4.0.0",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-react-native": "^4.0.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "clamp": "^1.0.1",
    "cross-fetch": "^1.1.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "es6-promise": "^4.2.2",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "expo": "23.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "native-base": "^2.3.6",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.0.0",
    "react-apollo": "^2.0.4",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-23.0.0.tar.gz",
    "react-native-scripts": "^1.8.1",
    "react-native-vector-icons": "^4.4.3",
    "react-native-web": "^0.3.0",
    "react-router-dom": "^4.2.2",
    "react-router-native": "^4.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "main": "src/index.native.js",
  "scripts": {
    "start-web": "node scripts/start.js",
    "build-web": "node scripts/build.js",
    "test-web": "node scripts/test.js --env=jsdom",
    "eject-web": "node scripts/eject.js",
    "start-native": "react-native-scripts start",
    "eject-native": "react-native-scripts eject",
    "test-native": "node node_modules/jest/bin/jest.js --watch"
  },
  "devDependencies": {
    "jest-expo": "23.0.0",
    "react-test-renderer": "16.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "react-native"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
